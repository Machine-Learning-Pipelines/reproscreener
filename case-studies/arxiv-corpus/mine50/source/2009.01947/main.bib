journal = {Computers and Operations Research},
keywords = {Combinatorial optimization,Critical node detection,Heuristics,Integer linear programming,NP-complete},
number = {7},
pages = {2193--2200},
title = {{Detecting critical nodes in sparse graphs}},
volume = {36},
year = {2009}
}
