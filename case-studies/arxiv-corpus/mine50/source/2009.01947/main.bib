author = {Bilmes, Jeffrey and Bai, Wenruo},
eprint = {1701.08939},
file = {:home/alan/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Bilmes, Bai - 2017 - Deep Submodular Functions.pdf:pdf},
month = {jan},
title = {{Deep Submodular Functions}},
year = {2017}
